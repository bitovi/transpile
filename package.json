{
  "name" : "transpile",
  "description" : "Transpiles JavaScript modules from one format to another.",
  "version" : "0.8.0-pre.1",
  "author" : {
    "name" : "Bitovi",
    "email" : "contact@bitovi.com",
    "web" : "http://bitovi.com/"
  },
  "dependencies": {
    "babel-core": "~4.1.1",
    "comparify": "0.1.0",
    "escodegen": "1.3.2",
    "esprima": "1.1.1",
    "estraverse": "1.5.0",
    "js-module-formats": "~0.1.2",
    "js-string-escape": "1.0.0",
    "sourcemap-to-ast": "0.0.2",
    "traceur": "0.0.87"
  },
  "devDependencies": {
    "chai": "*",
    "istanbul": "*",
    "mocha": "1.18.2",
    "xunit-file": "*"
  },
  "globalBrowser": {
    "traceur#./src/node/traceur.js": "./browser/traceur.js"
  },
  "system": {
    "meta": {
      "traceur#./bin/traceur": {
        "format": "global"
      }
    }
  },
  "scripts": {
    "mocha": "mocha test/*.js",
    "test": "./node_modules/istanbul/lib/cli.js cover -- ./node_modules/.bin/_mocha test/*.js --reporter spec"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.php"
    }
  ],
  "main": "main"
}
